<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="400.0" style="-fx-background-color: #747474;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projetopdi.FXMLPopupController">
   <children>
      <HBox alignment="CENTER" prefHeight="300.0" prefWidth="400.0">
         <children>
            <VBox alignment="CENTER" prefHeight="300.0" prefWidth="400.0">
               <children>
                  <HBox alignment="CENTER" layoutX="10.0" layoutY="185.0">
                     <children>
                        <Label fx:id="popupLabel1" text="Label" textAlignment="CENTER" textFill="WHITE">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="popupInput1" prefWidth="100.0" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER" layoutX="10.0" layoutY="173.0">
                     <children>
                        <Label fx:id="popupLabel2" text="Label" textAlignment="CENTER" textFill="WHITE" visible="false">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="popupInput2" prefWidth="100.0" visible="false" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER" layoutX="10.0" layoutY="135.0">
                     <children>
                        <Label fx:id="popupLabel3" text="Label" textAlignment="CENTER" textFill="WHITE" visible="false">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="popupInput3" prefWidth="100.0" visible="false" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER">
                     <children>
                        <Label fx:id="popupLabel4" text="Label" textAlignment="CENTER" textFill="WHITE" visible="false">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="popupInput4" prefWidth="100.0" visible="false" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </HBox>
                  <Button fx:id="popupBtnOk" mnemonicParsing="false" onAction="#onClickOk" prefWidth="120.0" style="-fx-background-color: #2A2A2A; -fx-text-fill: white;" text="OK">
                     <VBox.margin>
                        <Insets top="30.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
